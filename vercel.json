{
    "version": 2,
    "builds": [
        {
            "src": "index.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "(.*)",
            "dest": "index.ts"
        }
    ],
    "rewrites": [
        {
            "source": "/:path*",
            "destination": "/index.html"
        }
    ]
}
